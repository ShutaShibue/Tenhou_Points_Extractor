{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 21",
    "_version": "21.0.0",
  
    "compilerOptions": {
      "lib": ["ES2022"],
      "module": "node16",
      "target": "es2022",
  
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node16",
      "outDir": "./dist",
    },
    "include": [
      "src"
    ]

  }